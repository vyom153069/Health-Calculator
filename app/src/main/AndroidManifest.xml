<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vyomchandra.com.healthcalculator">

    <application
        android:allowBackup="true"
        android:icon="@drawable/healthlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/healthlogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
    <activity
        android:name=".notes"
        android:label="@string/title_activity_notes"

        ></activity>
    <activity android:name=".healthActivity" />
    <activity android:name=".BPFcalculator" />
    <activity android:name=".BMIcalculator" />
    <activity android:name=".BMRCalculator" />
    <activity android:name=".homeactivity"
        />
    <activity
        android:name=".MainActivity"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:name=".EditNoteActivity"
        android:parentActivityName=".notes" />
    </application>

</manifest>